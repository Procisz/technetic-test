{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "description": "Technetics",
  "author": "Agoston ZAJKAS",
  "engines": {
    "node": ">=16.13.2"
  },
  "scripts": {
    "ng": "ng",
    "setup": "npm run clear:folders && npm i && npm audit fix",
    "start": "npm run configEnv -- --environment=dev && npm run serve && npm run serve",
    "serve": "ng serve",
    "build": "npm run configEnv -- --environment=prod && ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "configEnv": "ts-node ./setEnv.ts",
    "clear:folders": "rm -rf node_modules && rm -rf dist && rm -rf .angular"
  },
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/cdk": "^15.0.1",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/material": "^15.0.1",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "dotenv": "^16.0.3",
    "rxjs": "~7.5.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.3.0",
    "yargs": "^17.6.2",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.2",
    "@angular/cli": "~15.0.2",
    "@angular/compiler-cli": "^15.0.0",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.8.2",
    "dotenv": "^16.0.3"
  }
}
